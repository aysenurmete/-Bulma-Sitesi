
@{
    ViewBag.Title = "SehirEkle";
}

@using OnlineIs.Models
<div class="row">
    <div class="col-md-12">
        <div class="block">
            <div class="block-heading">
                <div class="main-text h2">
                    Şehir Ekleme Sayfası
                </div>
            </div>
            <div class="block-content-outer">
                <div class="block-content-inner">
                    <form role="form" action="/Sehir/SehirEkle" method="post">
                        <div class="form-group">
                            <label for="exampleInputEmail1">
                                Şehir Adı
                            </label>
                            <input type="text" name="sehirADI" class="form-control" id="exampleInputEmail1" placeholder="Şehir Adı Giriniz">
                        </div>
                      
                        <button type="submit" class="btn btn-success">Kaydet</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>



