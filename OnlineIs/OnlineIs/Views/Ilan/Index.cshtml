
@{
    ViewBag.Title = "Index";
}
@using OnlineIs.Models;

@model List<ILAN>

<div class="col-md-12">
    <div class="block">
        <div class="block-heading">
            <div class="main-text h2">
                ILANLAR
            </div>
        </div>
        <div class="block-content-outer">
            <div class="block-content-inner">
                <table class="table table-striped table-hover">
                    <tbody>
                        @foreach (ILAN i in Model)
                        {
                            <tr>
                                <td>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="block">
                                                <div class="block-heading">
                                                    <div class="main-text h2">
            @i.ilanADI
        </div>
                                                    <div class="block-controls">
                                                        <a href="/Ilan/Index"> <span aria-hidden="true" class="icon icon-cross icon-size-medium block-control-remove"></span> </a>
                                                        <span aria-hidden="true" class="icon icon-arrow-down icon-size-medium block-control-collapse"></span>
                                                    </div>
                                                </div>
                                                <div class="block-content-outer">
                                                    <div class="block-content-inner">
                                                        <form role="form" action="/Ilan/Index" method="post">
                                                            <div class="form-group">

                                                                <label for="exampleInputPassword1">@i.DEPARTMAN.departmanADI</label>

                                                            </div>
                                                            <div class="form-group">
                                                                <label for="exampleInputPassword1">@i.POZISYON.pozisyonADI</label>

                                                            </div>
                                                            <div class="form-group">
                                                                <label for="exampleInputPassword1">@i.CALISMASEKLI.calismaADI</label>

                                                            </div>
                                                            <div class="form-group">
                                                                <label for="exampleInputPassword1">@i.EGITIMSEVIYESI.egitimseviyeADI</label>

                                                            </div>
                                                            <div class="form-group">
                                                                <label for="exampleInputPassword1">@i.YABANCIDIL.yabancidilADI</label>

                                                            </div>
                                                            <div class="form-group">

                                                                <label for="exampleInputPassword1">@i.nitelikler</label>

                                                            </div>

                                                            <button type="button" class="btn btn-primary">Başvur</button>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </td>

                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


